r;`\n      : `url(${unlikeButton}) no-repeat center center;`} \n  background-size: cover;\n  &:hover{{\n    cursor: pointer\n  }}\n  &:active{{\n    transform: translate(2px, 2px)\n  }}\n  `;\n\nconst Crawl = styled.div`\n  position: relative;\n  overflow: hidden;\n  font-size: 80%;\n  margin-top: 20px;\n`;\n\nconst Fadeout = styled.div`\n  position: absolute;\n  bottom: 0;\n  width: 100%;\n  height: 50px;\n  background: -webkit-linear-gradient(\n    rgba(40, 39, 39, 0) 0%,\n    rgba(40, 39, 39, 1) 100%\n  );\n`;\n\nconst FilmCard = (props) => {\n  const {\n    title,\n    episode_id,\n    opening_crawl,\n    director,\n    producer,\n    release_date,\n    movieImage,\n    favorite,\n  } = props.film;\n\n  const { favoriteToggle } = props;\n\n  // clicking on card takes user to film page\n\n  return (\n    <Card>\n      <h4>{title}</h4>\n      <div>Release Date: {release_date}</div>\n      <div>Episode: {episode_id}</div>\n      <div>Director: {director}</div>\n      <div>Producer: {producer}</div>\n      <Crawl>\n        {opening_crawl}\n        <Fadeout></Fadeout>\n      </Crawl>\n      <div>{movieImage}</div>\n      <Button favorite={favorite} id={episode_id} onClick={favoriteToggle} />\n    </Card>\n  );\n};\n\nexport default FilmCard;\n","/home/tal/Code/starwars/src/App.js",[],"/home/tal/Code/starwars/src/services/filmServices.js",[],"/home/tal/Code/starwars/src/components/WelcomeModal.js",[],"/home/tal/Code/starwars/src/services/modalServices.js",[],"/home/tal/Code/starwars/src/consts.js",[],"/home/tal/Code/starwars/src/pages/CardContainer.js",[],"/home/tal/Code/starwars/src/services/ApiServices.js",[],"/home/tal/Code/starwars/src/components/NavBar.js",[],"/home/tal/Code/starwars/src/styles/genericStyles.js",[],"/home/tal/Code/starwars/src/components/PageNotFound.js",[],{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":4,"column":10,"nodeType":"62","messageId":"63","endLine":4,"endColumn":25}